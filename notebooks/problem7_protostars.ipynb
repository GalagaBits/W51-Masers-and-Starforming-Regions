   "source": [
    "def dimensionless_mass(c_s, P_s, M):\n",
    "    top = M\n",
    "    bot = (c_s ** 4) / np.sqrt((const.G**3) * P_s)\n",
    "    \n",
    "    return top/bot"
   ]
